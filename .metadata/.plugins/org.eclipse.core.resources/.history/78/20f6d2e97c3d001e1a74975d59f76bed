package com.osttra;

import java.util.HashMap;
import java.util.Map;
import java.util.Set;

public class Mapp {
	
	public static void main(String[] args) {
		
		Map<Integer, String> map = new HashMap<>();
		
		map.put(100, "Anmol");
		map.put(102, "Naman");
		map.put(101, "Gaurav");
		
		Set<Integer> set =  map.keySet();
		
		for(int value : set) {
			
			System.out.println(map.get(value));
		}
		
		
	}
}


