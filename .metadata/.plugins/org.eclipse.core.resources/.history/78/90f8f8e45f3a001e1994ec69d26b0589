
public class First {
	
	public static void main(String[] args) {
		
		int i = 4, j = 5;
		
		First first = new First();
		
		first.add(i, j);
			
	}
	
	void add(int i, int j) {
		
		System.out.println(i+j);
		
		
	}

}


