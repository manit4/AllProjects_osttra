package com.osttra;

import java.util.ArrayList;

public class Collectionn {
	
	public static void main(String[] args) {
		
		ArrayList list = new ArrayList();
		
		list.add(34);
		list.add(12);
		list.add("Khush");
		
		for( int i = 0; i < list.size(); i++) {
			
			System.out.println(list.get(i));
		}
	}

}

