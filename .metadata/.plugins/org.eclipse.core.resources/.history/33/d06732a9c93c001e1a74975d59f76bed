package com.osttra;

import java.util.ArrayList;

public class Collectionn {
	
	public static void main(String[] args) {
		
		ArrayList list = new ArrayList();
		
		list.add(34);
		list.add(12);
		list.add("Khush");
		
		list.add(1, "Parv");
		
		list.set(0, "Anmol");
		
		System.out.println(list);
	}
}


